        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api/",
            "description": "Returns App API"
        }
    ],
    "paths": {
        "/": {},
        "/v1/Register1": {
            "post": {
                "tags": [
                    "Register"
